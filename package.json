{
  "name": "vuedash-ui",
  "version": "0.1.3",
  "private": false,
  "main": "./dist/vuedash.common.js",
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js",
    "components/*",
    "common/*"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
